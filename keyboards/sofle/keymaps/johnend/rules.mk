# Enable system and audio controls
EXTRAKEY_ENABLE = yes
# Disables console for debugging
CONSOLE_ENABLE = no
RGBLIGHT_ENABLE = yes
ENCODER_ENABLE = yes
# Increases compile time but reduces size
LTO_ENABLE = yes
OLED_ENABLE = yes
# Caps words toggle
CAPS_WORD_ENABLE = yes
# VIA support - enable this if using standard QMK with VIA
# Note: For VIAL support, you need to use the VIAL fork of QMK
# VIA_ENABLE = yes
